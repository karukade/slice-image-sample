{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": ["dom", "ES2019", "DOM.Iterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "target": "ESNext",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["src"],
  "exclude": ["./node_modules/**/*"]
}